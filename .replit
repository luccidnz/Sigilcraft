entrypoint = "server.js"
modules = ["python-3.11", "nodejs-20"]

[nix]
channel = "stable-25_05"
packages = ["freetype", "lcms2", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "tcl", "tk", "zlib"]

[unitTest]
language = "python3"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[deployment]
run = ["sh", "-c", "python unified_server.py"]
deploymentTarget = "cloudrun"

[workflows]
runButton = "Sigilcraft Production"

[[workflows.workflow]]
name = "Run"
author = 45492115
mode = "parallel"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node server.js"

